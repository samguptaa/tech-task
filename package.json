{
    "name": "fabacus-reservation-service",
    "version": "1.0.0",
    "description": "Event seat reservation service using Node.js, Redis, and Docker",
    "type": "module",
    "main": "src/app.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/app.js",
        "dev": "ts-node src/app.ts",
        "dev:watch": "nodemon --exec ts-node src/app.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "type-check": "tsc --noEmit"
    },
    "keywords": [
        "reservation",
        "seats",
        "events",
        "redis",
        "nodejs"
    ],
    "author": "Fabacus Team",
    "license": "MIT",
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "express-rate-limit": "^6.10.0",
        "helmet": "^7.0.0",
        "joi": "^17.9.2",
        "node-fetch": "^3.3.2",
        "pg": "^8.16.3",
        "redis": "^4.6.8",
        "uuid": "^9.0.0",
        "winston": "^3.10.0",
        "ws": "^8.18.3"
    },
    "devDependencies": {
        "@types/cors": "^2.8.15",
        "@types/express": "^4.17.20",
        "@types/jest": "^29.5.6",
        "@types/node": "^20.8.0",
        "@types/pg": "^8.15.5",
        "@types/supertest": "^2.0.15",
        "@types/uuid": "^9.0.5",
        "@types/ws": "^8.18.1",
        "jest": "^29.6.2",
        "nodemon": "^3.0.1",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
